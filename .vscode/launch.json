{
    "configurations": [
        {
            "name": "Launch Chrome",
            "request": "launch",
            "type": "chrome",
            "url": "https://local-teacherstory.com",
            "webRoot": "${workspaceFolder}",
            "runtimeArgs": ["--ignore-certificate-errors"]
        },
        {
            "name": "Launch Firefox",
            "type": "firefox",
            "request": "launch",
            "reAttach": true,
            "url": "https://local-teacherstory.com",
            "profile": "dev",
            "keepProfileChanges": true,
            "skipFiles": ["*"]
        }
    ]
}